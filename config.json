{
  "database": {
    "Dialect"      : "mysql",
    "Database"     : "filesync",
    "User"	       : "root",
    "Password"     : "123456",
    "Charset"      : "utf8mb4",                 /*要支持emoji的话，填 utf8mb4 */
    "Host"         : "127.0.0.1",               /*数据库ip*/
    "Port"         : 3306,                      /*数据库端口*/
    "MaxIdleConns" : 5,                         /*空闲时最大的连接数*/
    "MaxOpenConns" : 10                         /*最大的连接数*/
  },
  "go": {
    "WebPort"             : 8023,               /*Web服务器监听端口*/
    "FServerPort"         : 8686,               /*文件传输服务器监听端口*/
    "MaxMultipartMemory"  : 3,                  /*上传的图片最大允许的大小，单位MB*/
    "MaxFtsNum"           : 10,                  /*每次最大同时传输文件个数*/
    "Env"                 : "development",      /*模式(开发，测试，产品)*/
    "LogDir"              : "",                 /*日志文件所在的目录，如果不设的话，默认在项目目录下*/
    "LogFile"             : "filesync.log"      /*日志文件*/
  }
}